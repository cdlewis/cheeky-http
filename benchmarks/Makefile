middleware:
	@./run.sh 1 middleware
	@./run.sh 5 middleware
	@./run.sh 10 middleware
	@./run.sh 15 middleware
	@./run.sh 20 middleware
	@./run.sh 30 middleware
	@./run.sh 50 middleware
	@./run.sh 100 middleware
	@echo

routes:
	@./run.sh 1 routes
	@./run.sh 10 routes
	@./run.sh 100 routes
	@./run.sh 1000 routes
	@./run.sh 10000 routes
	@./run.sh 100000 routes
	@echo

.PHONY: all
